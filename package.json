{
  "name": "payuni-ts-sdk",
  "version": "1.0.1",
  "publishConfig": {
    "access": "public"
  },
  "description": "TypeScript SDK for PAYUNi payment gateway integration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solomeowl/PAYUNi_TS_SDK.git"
  },
  "keywords": [
    "payuni",
    "payment",
    "gateway",
    "typescript",
    "sdk"
  ],
  "author": "solomeowl",
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "examples"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "bugs": {
    "url": "https://github.com/solomeowl/PAYUNi_TS_SDK/issues"
  },
  "homepage": "https://github.com/solomeowl/PAYUNi_TS_SDK#readme",
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.34.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "crypto-js": "^4.2.0"
  }
